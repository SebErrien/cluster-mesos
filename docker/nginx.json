{
  "id": "/www/ngninx-hello",
  "instances": 1,
  "cpus": 0.2,
  "mem": 32,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "boune/nginx",
       "network": "BRIDGE",
       "privileged": true,
       "portMappings": [
         { "containerPort": 80, "protocol": "tcp" }
       ]
    }
  },
  "env": {
    "SERVICE_NAME": "nginx-hello",
    "SERVICE_TAGS": "www"
  },
  "constraints": [["hostname", "UNIQUE"]],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}